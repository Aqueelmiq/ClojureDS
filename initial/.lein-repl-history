(defn communist-plus
  "Adds up numbers.  To allow for equality the sum is always 10."
  [& xx]
  ; I think you'll nail this one.
(defn communist
[& xx]
10
)
(communist 10 20 30 40 50 60 70 80 90)
(defn extreme
[& xx]
(apply * xx))
(extreme 1 2 3 4 5 6 7 8)
(defn capitalist
[& xx]
(cond 
(< n 2) (apply + (cons (cons xx n) -2))
